{
    "name": "spatie/laravel-mailcoach-sendgrid-feedback",
    "description": "Process feedback for email campaigns sent using Sendgrid",
    "keywords": [
        "spatie",
        "laravel-mailcoach-sendgrid-feedback"
    ],
    "homepage": "https://mailcoach.app",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://satis.spatie.be"
        }
    ],
    "require": {
        "php": "^7.4",
        "illuminate/http": "^7.0|^8.0",
        "orchestra/testbench": "^5.0|^6.0",
        "spatie/laravel-mailcoach": "^2.9",
        "spatie/laravel-webhook-client": "^2.2"
    },
    "require-dev": {
        "ext-json": "*",
        "fzaninotto/faker": "^1.9",
        "friendsofphp/php-cs-fixer": "^2.16",
        "orchestra/testbench": "^5.0|^6.0",
        "symfony/var-dumper": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\MailcoachSendgridFeedback\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\MailcoachSendgridFeedback\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\MailcoachSendgridFeedback\\MailcoachSendgridFeedbackServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
