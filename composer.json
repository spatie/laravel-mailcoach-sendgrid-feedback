{
    "name": "spatie/laravel-mailcoach-sendgrid-feedback",
    "description": "Process feedback for email campaigns sent using Sendgrid",
    "keywords": [
        "spatie",
        "laravel-mailcoach-sendgrid-feedback"
    ],
    "homepage": "https://mailcoach.app",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://satis.spatie.be"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0",
        "symfony/sendgrid-mailer": "^6.0",
        "spatie/laravel-mailcoach": "^6.0"
    },
    "require-dev": {
        "ext-json": "*",
        "fakerphp/faker": "^1.12",
        "orchestra/testbench": "^7.0|^8.0",
        "phpunit/phpunit": "^9.5",
        "symfony/var-dumper": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\MailcoachSendgridFeedback\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\MailcoachSendgridFeedback\\Tests\\": "tests",
            "Spatie\\Mailcoach\\Database\\Factories\\": "vendor/spatie/laravel-mailcoach/database/factories"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\MailcoachSendgridFeedback\\MailcoachSendgridFeedbackServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
